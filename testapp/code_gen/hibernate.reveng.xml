<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="TINYINT" hibernate-type="boolean"
		not-null="true">
	</sql-type>
</type-mapping>
<table-filter match-catalog="testdb" match-name="address"/>
  <table-filter match-catalog="testdb" match-name="barcode"/>
  <table-filter match-catalog="testdb" match-name="category"/>
  <table-filter match-catalog="testdb" match-name="item"/>
  <table-filter match-catalog="testdb" match-name="parent"/>
  <table-filter match-catalog="testdb" match-name="product"/>
</hibernate-reverse-engineering>